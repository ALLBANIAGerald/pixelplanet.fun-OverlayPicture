{
    "name": "pixelplanet.fun-picture-overlay",
    "displayName": "pixelplanet.fun picture overlay",
    "version": "1.0.19",
    "description": "Add your picture as overlay to pixelplanet.fun",
    "main": "./dist/index.js",
    "scripts": {
        "build:prod": "rollup -c --environment PRODUCTION",
        "build": "rollup -c & npm run open",
        "build:watch": "rollup -c --watch",
        "build:verify": "rollup -c --environment VERIFY_BUILD",
        "open": "open-cli file://%cd%\\dist\\pixelPlanetOverlay.user.js -- firefox",
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --quiet --fix",
        "cleanNodeTypes": "rimraf .\\node_modules\\@types\\node",
        "postinstall": "npm run cleanNodeTypes && husky install",
        "prepare": "husky install"
    },
    "keywords": [
        "pixelplanet.fun",
        "pixelplanet",
        "template",
        "picture",
        "overlay"
    ],
    "author": "Woyken",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.17.2",
        "@open-wc/rollup-plugin-html": "^1.0.2",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.7",
        "autoprefixer": "^10.4.2",
        "clean-css": "^5.2.4",
        "husky": "^5.1.3",
        "node-sass": "^7.0.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.7.3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-peer-deps-external": "^2.2.2",
        "rollup-plugin-replace": "^2.2.0",
        "rollup-plugin-scss": "^3.0.0",
        "rollup-plugin-terser": "^5.3.0",
        "rollup-plugin-typescript2": "^0.27.0",
        "rollup-plugin-web-worker-loader": "^1.6.1",
        "sass": "^1.49.7",
        "ts-loader": "^7.0.1",
        "tslint-config-airbnb": "^5.11.2",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "@material-ui/core": "^4.9.12",
        "@material-ui/icons": "^4.9.1",
        "@typescript-eslint/eslint-plugin": "^5.11.0",
        "@typescript-eslint/parser": "^5.11.0",
        "comlink": "^4.3.0",
        "eslint": "^8.8.0",
